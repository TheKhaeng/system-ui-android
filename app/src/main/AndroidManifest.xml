<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thekhaeng.systemui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".insets.BasicActivity"
            android:theme="@style/Theme.Basic"/>

        <activity android:name=".insets.ConsumedInsetsActivity"
            android:theme="@style/Theme.DrawStatusBars"/>

        <activity android:name=".insets.ConsumedInsetsCorrectActivity"
            android:theme="@style/Theme.DrawStatusBars"/>

        <activity android:name=".insets.UnconsumedInsetsActivity"
            android:theme="@style/Theme.DrawStatusBars"/>

        <activity android:name=".insets.UnconsumedInsetsCorrectActivity"
            android:theme="@style/Theme.DrawStatusBars"/>
    </application>

</manifest>